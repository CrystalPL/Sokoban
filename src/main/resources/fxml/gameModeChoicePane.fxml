            <font>
                <Font name="System Bold Italic" size="17.0"/>
            </font>
        </Button>
        <Button layoutX="612.0" layoutY="300.0" mnemonicParsing="false" onAction="#randomLevel" prefHeight="79.0"
                prefWidth="234.0" text="Losowy poziom" textFill="WHITE">
