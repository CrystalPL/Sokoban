<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="820.0" prefWidth="1457.0"
            stylesheets="/css/button.css"
            style="-fx-background-image: url('/img/gui/background/background.png')"
            xmlns="http://javafx.com/javafx/8.0.202-ea"
            fx:controller="pl.crystalek.sokoban.controller.GameModeChoiceController">
    <children>
        <Button layoutX="612.0" layoutY="520.0" mnemonicParsing="false" onAction="#ownMap" prefHeight="79.0"
